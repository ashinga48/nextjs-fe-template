{
  "name": "serviceprovider-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
    "build:analyze": "ANALYZE=true npm run build",
    "apollo:download-schema": "apollo client:download-schema schema.graphql",
    "apollo:generate-types": "rm -rf __generated__ && apollo client:codegen --target typescript --outputFlat",
    "apollo:sync": "npm run apollo:download-schema && npm run apollo:generate-types",
    "test": "jest -c test/jest.config.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --config video=false",
    "cypress:test": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@next/bundle-analyzer": "^10.0.5",
    "axios": "^0.21.1",
    "graphql": "^15.4.0",
    "next": "10.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-translate": "^1.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-use": "^15.3.8",
    "styled-components": "^5.2.1",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.0",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "apollo": "^2.32.1",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "^6.2.1",
    "dotenv": "^8.2.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^26.6.3",
    "nock": "^13.0.6",
    "start-server-and-test": "^1.11.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
